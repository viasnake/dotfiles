Include conf.d/config.secret

SetEnv TERM=xterm-256color

host alflag
  HostName ssh.alflag.internal
  Port 22
  User infra
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host github
  HostName github.com
  User git
  ForwardAgent yes
  IdentityFile ~/.ssh/snake

Host github.com
  HostName github.com
  User git
  ForwardAgent yes
  IdentityFile ~/.ssh/snake
